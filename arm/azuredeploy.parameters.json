{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "@LOCATION@"
    },
    "vmssName": {
      "value": "@VMSS_NAME@"
    },
    "vmSku": {
      "value": "@VM_SKU@"
    },
    "adminUsername": {
      "value": "@ADMIN_USERNAME@"
    },
    "instanceCount": {
      "value": "@INSTANCE_COUNT@"
    },
    "existingManagedImageResourceGroup": {
      "value": "@PACKER_RESOURCE_GROUP@"
    },
    "existingManagedImageName": {
      "value": "@IMAGE_NAME@"
    },
    "singlePlacementGroup": {
      "value": "true"
    },
    "pipName": {
      "value": "@PUBLIC_IP_NAME@"
    },
    "pipLabel": {
      "value": "@PUBLIC_IP_LABEL@"
    },
    "skuType": {
      "value": "Standard"
    },
    "ipAllocationMethod": {
      "value": "Static"
    },
    "priority": {
      "value": "Regular"
    },
    "adminPassword": {
      "value": "@ADMIN_PASSWORD@"
    }
  }
}